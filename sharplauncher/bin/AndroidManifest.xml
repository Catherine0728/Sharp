        <activity android:name="com.sharp.sharplauncher.activity.TvListBackActivity" >
        </activity>
        <activity android:name="com.sharp.sharplauncher.activity.SettingActivity" >
        </activity>
        <activity android:name="com.sharp.sharplauncher.activity.VideoCarouselActivity" >
